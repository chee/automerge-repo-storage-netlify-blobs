{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"declaration": true,
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"allowImportingTsExtensions": true,
		"emitDeclarationOnly": true
	},
	"files": ["source/adapter.ts"]
}
